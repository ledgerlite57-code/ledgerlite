NODE_ENV=production

# API
API_PORT=4000
API_JWT_SECRET=replace_with_long_random_secret
API_JWT_REFRESH_SECRET=replace_with_long_random_secret
API_JWT_ACCESS_TTL=900
API_JWT_REFRESH_TTL=1209600
EMAIL_VERIFICATION_TTL_HOURS=24
API_CORS_ORIGIN=https://dev-app.example.com
WEB_BASE_URL=https://dev-app.example.com
SMTP_DISABLE=true
INVENTORY_COST_EFFECTIVE_DATE_ENABLED=true
INVENTORY_COST_HIGH_PRECISION_QTY_ENABLED=true
NEGATIVE_STOCK_POLICY_ENABLED=true
INVITE_LIFECYCLE_ENABLED=true
ONBOARDING_CHECKLIST_ENABLED=true
SENTRY_DSN=
SENTRY_ENVIRONMENT=development
API_SWAGGER_ENABLED=true
API_SWAGGER_PATH=docs
API_SWAGGER_REQUIRE_AUTH=false
API_SWAGGER_AUTH_TOKEN=

# DB (same password as db service in docker-compose.development.yml)
POSTGRES_PASSWORD=replace_with_db_password
DATABASE_URL=postgresql://ledgerlite:replace_with_db_password@db:5432/ledgerlite_dev

# WEB
NEXT_PUBLIC_API_BASE_URL=https://dev-api.example.com
NEXT_PUBLIC_APP_VERSION=dev-local
NEXT_PUBLIC_ENVIRONMENT_LABEL=DEV
NEXT_PUBLIC_NON_PROD_SAFETY_BANNER_ENABLED=true
